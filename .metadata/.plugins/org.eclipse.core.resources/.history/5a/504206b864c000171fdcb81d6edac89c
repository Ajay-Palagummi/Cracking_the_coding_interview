public static boolean oneaway(String a,String b){
	if (a.length() > b.length()){
		if (a.length()-b.length() > 2){
			return false;
		}
		else{
			return isoneaway(a,b);
		}
	}
	else if (b.length() > a.length()){
		if(b.length()-a.length() > 2){
			return false;
		}
		else{
			return isoneaway(b,a);
		}
	}
	else {
		return isoneaway(a,b);
	}
}

